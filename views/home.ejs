<%- include("partials/header")%>
<h1>Home</h1>
<p><%= homeText %></p>
<div>
  <% posts.forEach(post => { %>
  <h2><%= post.title %></h2>
  <p>
    <%= post.content.length > 100 ? post.content.substring(0,100) + ' ...' : post.content %>
    <a href='/posts/<%= post._id %> ' >Read More</a>
  </p>
  <% }) %>
</div>
<%- include("partials/footer")%>
