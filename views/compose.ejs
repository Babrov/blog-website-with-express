<%- include("partials/header")%>
<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input type="text" name="postTitle" class="form-control" />
    <label for="postText">Post</label>
    <textarea class="form-control" name="postText" rows="3"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer")%>
